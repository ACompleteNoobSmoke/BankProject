    "version": "0.2.0",
    "configurations": [
        {
            "name": "Java",
            "type": "java",
            "request": "launch",
            "stopOnEntry": true,
            "jdkPath": "${env:JAVA_HOME}/bin",
            "cwd": "${fileDirname}",
            "startupClass": "${fileBasenameNoExtension}",
            "classpath": [
                ".",
                "${fileDirname}"
            ]
        },
        {
            "name": "Java Console App",
            "type": "java",
            "request": "launch",
            "stopOnEntry": true,
            "jdkPath": "${env:JAVA_HOME}/bin",
            "cwd": "${fileDirname}",
            "startupClass": "${fileBasenameNoExtension}",
            "classpath": [
                ".",
                "${fileDirname}"
            ],
            "externalConsole": true
        },
        {
            "type": "java",
            "name": "Debug (Launch) - Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Debug (Launch)-BankingApplication<BankProject>",
            "request": "launch",
            "mainClass": "Program.BankingApplication",
            "projectName": "BankProject"
        },
        {
            "type": "java",
            "name": "Debug (Launch)-DocsConnectionPropsHelper<BankProject>",
            "request": "launch",
            "mainClass": "com.mysql.jdbc.DocsConnectionPropsHelper",
            "projectName": "BankProject"
        },
        {
            "type": "java",
            "name": "Debug (Launch)-ErrorMappingsDocGenerator<BankProject>",
            "request": "launch",
            "mainClass": "com.mysql.jdbc.util.ErrorMappingsDocGenerator",
            "projectName": "BankProject"
        },
        {
            "type": "java",
            "name": "Debug (Launch)-PropertiesDocGenerator<BankProject>",
            "request": "launch",
            "mainClass": "com.mysql.jdbc.util.PropertiesDocGenerator",
            "projectName": "BankProject"
        },
        {
            "type": "java",
            "name": "Debug (Launch)-TimezoneDump<BankProject>",
            "request": "launch",
            "mainClass": "com.mysql.jdbc.util.TimezoneDump",
            "projectName": "BankProject"
        },
        {
            "type": "java",
            "name": "Debug (Launch)-VersionFSHierarchyMaker<BankProject>",
            "request": "launch",
            "mainClass": "com.mysql.jdbc.util.VersionFSHierarchyMaker",
            "projectName": "BankProject"
        }
    ]
